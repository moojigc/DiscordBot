{
  "name": "intellidnd",
  "version": "2.0.0",
  "description": "A cross-platform application to help you manage your D&D character.",
  "main": "server/src/index.js",
  "engines": {
    "node": "12.13.0",
    "npm": "6.12.0"
  },
  "scripts": {
    "sass": "ts-node client/src/assets/scss/variable_compiler.ts",
    "sass:watch": "nodemon --exec ts-node client/src/assets/scss/variable_compiler.ts --watch client/App.scss",
    "install": "cd client && npm install",
    "client": "cd client && npm start",
    "server": "nodemon --exec ts-node --project ./tsconfig.server.json server/index.ts --watch server -e *.ts",
    "open": "code . & npm run start",
    "start": "concurrently \"npm run server\" \"npm run client\" \"npm run sass:watch\"",
    "bot": "nodemon --exec ts-node --project ./tsconfig.bot.json bot/index.ts --watch bot -e *.ts",
    "build": "concurrently \"tsc --project ./tsconfig.server.json\" \"tsc --project ./tsconfig.bot.json && cd client && npm run build\"",
    "build:bot": "tsc --project ./tsconfig.bot.json",
    "copy": "cp -r bot/* ../temp/intellidnd-bot/bot",
    "cleanup": "find . -name \"*.js\" -type f -delete",
    "test": "echo \"No test\" && exit 1",
    "start:prod": "node build/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moojigc/DiscordBot.git"
  },
  "author": "M. Moojig Battsogt",
  "license": "ISC",
  "dependencies": {
    "@tsconfig/node12": "^1.0.7",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-mongodb-session": "^2.3.2",
    "cors": "^2.8.5",
    "discord.js": "^12.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.4",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.9.18",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "ts-node-dev": "^1.0.0-pre.57"
  },
  "bugs": {
    "url": "https://github.com/moojigc/intellidnd/issues"
  },
  "homepage": "https://github.com/moojigc/intellidnd#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/connect-mongodb-session": "0.0.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.158",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.7.32",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.26",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "concurrently": "^5.2.0",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.2"
  },
  "keywords": [
    "dnd",
    "inventory",
    "discord",
    "bot"
  ]
}